<Window x:Class="MD5_Test2.iniWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MD5_Test2"
        mc:Ignorable="d"
        Title="iniWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Width="200"
                    Height="60" 
                    Content="写ini文件"
                    Click="Button_Click" />
            <Button Width="200"
                    Height="60"
                    Content="ini生成md5文件"
                    Click="Button_Click_1" />
            <Button Width="200"
                    Height="60"
                    Content="校验两个文件"
                    Click="Button_Click_2" />
        </StackPanel>
    </Grid>
</Window>
